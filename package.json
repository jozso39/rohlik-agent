    "@langchain/tavily": "^0.1.5",
    "dotenv": "^17.2.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
