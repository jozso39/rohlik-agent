{
  "name": "langgraph-js-sample",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "dist/main.js",
  "scripts": {
    "build": "swc . -d dist --ignore node_modules,docs,dist --quiet",
    "build:ts": "tsc --build --verbose",
    "demo": "swc . -d dist --ignore node_modules,docs,dist --quiet && node dist/main.js",
    "chat": "swc . -d dist --ignore node_modules,docs,dist --quiet && node dist/src/interactive.js",
    "dev:demo": "npx tsx main.ts",
    "dev:chat": "npx tsx src/interactive.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:check": "eslint . --ext .ts --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:ts": "prettier --write \"**/*.ts\"",
    "check": "npm run lint:check && npm run format:check",
    "fix": "npm run lint:fix && npm run format",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@langchain/langgraph": "^0.4.6",
    "@langchain/openai": "^0.6.9",
    "dotenv": "^17.2.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}